{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "aspnet-signalr@1.1.4",
      "destination": "wwwroot/lib/aspnet-signalr/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@3.1.4",
      "destination": "wwwroot/lib/@microsoft/signalr/"
    },
    {
      "library": "signalr.js@2.4.1",
      "destination": "wwwroot/lib/signalr.js/"
    },
    {
      "provider": "unpkg",
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/",
      "files": [
        "dist/jquery.unobtrusive-ajax.min.js"
      ]
    }
  ]
}